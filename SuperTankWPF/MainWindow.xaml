<Window x:Class="SuperTankWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SuperTankWPF"
        mc:Ignorable="d"
        Title="Battle City"
        Height="{Binding Source={x:Static local:ConfigurationWPF.WindowClientHeight}}"
        Width="{Binding Source={x:Static local:ConfigurationWPF.WindowClientWidth}}"
        Icon="Content\Ico\Ico.ico"
        Loaded="Window_Loaded" KeyUp="Window_KeyUp">
    <Grid>
        <local:StartScren x:Name="startScren"/>
    </Grid>
</Window>
